@inherits Umbraco.Web.Macros.PartialViewMacroPage

@if(Model.MacroParameters["youtubeId"] != null)
{
    var youtubeId = Model.MacroParameters["youtubeId"] as string;
    
    if(!string.IsNullOrEmpty(youtubeId))
    {
        <div class="row">
            <div class="col-xs-6 col-md-6">
                <iframe width="960" height="600" src="https://www.youtube.com/embed/@youtubeId"></iframe>
            </div>
        </div>
    }

}